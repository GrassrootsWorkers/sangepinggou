    float: left;
    margin-left: 30px;
    margin-top: 20px;
}
.login_bottom{
    margin: auto;
    text-align: right;
    width: 400px;
    line-height: 30px;
    word-wrap: break-word;
    word-break: normal;
}
.login_bottom a{
    font-size: 14px;
    color: #3e98d0;
    word-wrap: break-word;
    word-break: normal;
    float: right;
}
.remem_pass_con{
    padding:15px 0 35px 30px;
}

.remem_pass{
    font-size: 14px;
    color:#aabeca;
    width: 200px;
    float: left;
}
.login_dx a{
    color: #5c5c5c;
    float: right;
    font-size: 14px;
    margin-right: 10px;
}
.forget_cont{
    font-size: 12px;
    color:#7d7d7d;
    width:300px;
    padding: 10px 0 12px;
}
.forget_cont input[type="checkbox"]{
    margin: 2px 2px 0;
}
.forget_cont a{
    color:#7d7d7d;
}
.forget_pass{
float: left;
}
.forget_resget{
    float: right;
    right: 5px;
}
.footer{
    bottom: 0;
    clear: both;
    width: 100%;
    position: relative;
    margin-top: 30px;
}
.copyright{
    font-size: 14px;
    color: #666;
    clear: both;
    padding: 20px 0;
}
.copyright p{
    color: #b2b2b2;
    text-align: center;
    padding-top: 0;
    font-size: 14px;
}

