/*******注册成功弹框样式开始********/
.bg_fixed_register,.bg_fixed_re_pass,.bg_fixed_login{
    background: rgba(0,0,0,0.5) none repeat scroll 0 0;
    background: url(../images/feedback_modal_bg.png) repeat \9;
    height: 100%;
    left: 0;
    position: fixed;
    display: none;
    top: 0;
    width: 100%;
    z-index: 100;
}

.win_box_register,.win_box_re_pass,.win_box_login{
    top:50%;
    margin-top:-90px;
    left: 50%;
    margin-left:-205px;
    width:365px;
    height:180px;
    background: #fff;
    display: none;
    border-radius: 5px;
    position: relative;
    padding-left: 45px;
}
.win_cont_register,.win_cont_re_pass,.win_cont_login{
    padding-top: 30px;
    padding-right: 40px;
}
.win_box_register .bottom_btn,.win_box_re_pass .bottom_btn,.win_box_login .bottom_btn{
    position: absolute;
    bottom: 25px;
}
.win_box_register .bottom_btn a.cancel,.win_box_login .bottom_btn a.cancel{
    border-radius: 0;
    border:1px solid #d0ceca;
    color:#d0ceca;
    width: 150px;
    height: 40px;
    line-height: 40px;
    display: block;
    background: #fff;
    text-align: center;
    float: left;
    margin-right: 10px;
    cursor: pointer;
}
.win_box_register .bottom_btn a.sure,.win_box_re_pass .bottom_btn a.sure,.win_box_login .bottom_btn a.sure{
    border-radius: 0;
    color:#fff;
    width: 150px;
    height: 40px;
    line-height: 40px;
    display: block;
    background: #ffaf23;
    text-align: center;
    float: left;
    cursor: pointer;
}

.win_cont_register span,.win_cont_re_pass span,.win_cont_login span{
    float: left;
    font-size: 16px;
    color:#444444;
}
.win_cont_register span.win_icon{
    background:url("../images/win_cont_register.png") no-repeat;
    width: 58px;
    height: 58px;
    display: block;
    margin-left: 25px;
    margin-right: 16px;
    margin-top: 8px;
}

.win_cont_register span.win_tit{
    margin-top:10px;
}
.win_close_register,.win_close_re_pass,.win_close_login{
    background: url("../images/win_close_icon.png");
    width:16px;
    height:16px;
    float: right;
    position: relative;
    right: 10px;
    top: 10px;
    display: block;
    cursor: pointer;
}
/*******注册成功弹框样式结束********/


/*******找回密码成功弹框样式结束********/

