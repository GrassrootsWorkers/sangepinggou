    margin-left: -15px;

}
.login-con{
   /* margin: auto;
    width: 360px;
    height:415px;
    background:url("../images/login-con.png") no-repeat ;
    padding-left: 56px;
    padding-top: 40px;*/
    margin: auto;
    width: 392px;
    height:436px;
    background: #fff;
    -webkit-box-shadow: #cfcfcf 0px 0px 8px;
    -moz-box-shadow: #cfcfcf 0px 0px 8px;
    box-shadow: #cfcfcf 0px 0px 8px;
    padding-left: 46px;
    padding-top: 35px;
    border-radius: 5px;
}
.login_con_bg{
    background: #fff;
    box-shadow: 0 0 8px #cfcfcf;
    -webkit-box-shadow: #cfcfcf 0px 0px 8px;
    -moz-box-shadow: #cfcfcf 0px 0px 8px;
    height: auto;
    margin: auto;
    padding-bottom: 35px;
    padding-top: 35px;
    border-radius: 5px;
    padding-left: 46px;
    width: 392px;
}
.login-con h1{
    color:#3e98d0;
    font-weight: normal;
    padding-bottom: 30px;
}

.input-list{
    display: block;
    height: 40px;
    margin-bottom: 13px;
    clear: both;
}

.input-list label{
    display: inline;
    float: left;
    font-size: 16px;
    margin-right: 20px;
    text-align: right;
    width:100px;
    line-height: 30px;
}
.input-list input {
    clear: both;
    color: #bdbab4;
    border: 0;
    height:42px;
    line-height:40px;
    vertical-align: baseline;
    padding-left: 50px;
    width: 300px;
    border:1px solid #bdbab4;
    border-radius: 5px;
    float: left;
    font-size: 14px;
}



.input-list input:hover,.input-list input:focus{
    border:1px solid #3e98d0;
}
.input_error{
    margin-bottom: 3px;
}
.input_error input{
    border:1px solid #ff5c5c;
}
.input-list .error{
    color: #ff5c5c;
    float: left;
    font-size: 12px;
    height: 15px;
    line-height: 20px;
    white-space: nowrap;
    width: 100%;
}
.input-list .error .error_icon{
    background:url("../images/error_icon.png") no-repeat ;
    height: 14px;
    width: 14px;
    display: block;
    margin-left: -20px;
    margin-top: 8px;
    position: absolute;
    float:left;
}
.header{
    height: 80px;
}
.yzm_img{
    float: left;
    margin-left: 10px;
    margin-top: 2px;
    vertical-align: middle;
}


img{
    max-width:100%;
}

.align-left{
    text-align:left;
}

.align-center{
    text-align:center;
}

.align-right{
    text-align:right;
}

img.align-left{
    text-align:left;
    float:left;
    margin:0 15px 15px 0;
}

img.align-center{
    text-align:center;
    margin:10px auto;
}

img.align-right{
    text-align:right;
    float:right;
    margin:0 0 15px 15px;
}

.login-btn [type=button]{
    background:#ffaf23;
    border:0;
    color:#fff;
    font-size: 24px;
    height: 40px;
    line-height: 38px;
    width: 300px;
    letter-spacing:3px;
    cursor: pointer;
    border-radius: 3px;
    font-weight: normal;
}
.login-hz{
    padding-top: 25px;
    clear: both;
    margin-left: 2px;
}
.login-hz i{
    background:url("../images/login_other_tit.png") no-repeat ;
    width: 298px;
    height: 14px;
    display: block;
}
.user_icon{
    background:url("../images/user_icon.png") no-repeat ;
    width: 20px;
    height: 22px;
    display: block;
    margin: 9px 12px;
    position: absolute;
}
.pass_icon{
    background:url("../images/pass_icon.png") no-repeat ;
