    height: 26px;
    margin: 8px 16px;
    position: absolute;
}
.yzm_input{
    float: left !important;
    padding-left: 10px !important;
    width: 188px !important;
}
.login-hz p{
    clear: both;
    float: left;
    font-size: 15px;
    padding-left: 116px;
}

.login-hz a.a-qq {
    background:url("../images/login_qq.png") no-repeat;
    width: 58px;
    height: 58px;
    display: block;
    float: left;
    margin-left: 80px;
    margin-top: 20px;
}

.login-hz a.a-wx {
    background:url("../images/login_weixin.png") no-repeat;
    width: 58px;
    height: 58px;
