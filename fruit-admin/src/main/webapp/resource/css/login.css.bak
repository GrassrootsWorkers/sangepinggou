}

.login_hand{
    width: 392px;
    margin: 200px auto auto;
}
.help-con{
     width: 392px;
 }
.login_hand h1{
    font-size: 36px;
    color:#3e98d0;
    font-weight: normal;
    text-align: center;
}
.login_hand a{
    font-size: 14px;
    color:#3e98d0
}
.login_hand div{
    margin-bottom: 7px;
}
.login_hand a:nth-child(2){
    float: left;
}
.login_hand a:last-child{
    float: right;
    text-align: right;
}
.login_hand a:last-child i{
    background:url("../images/login_phone_icon.png") no-repeat ;
    height: 14px;
    margin-top: 4px;
